                        <MenuItem mnemonicParsing="false" text="Paste" onAction="#paste" accelerator="Shortcut+v"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About" onAction="#about"/>
                    </items>
                </Menu>
            </menus>
