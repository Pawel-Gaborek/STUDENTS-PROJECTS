                    <effect>
                        <Blend mode="DIFFERENCE" opacity="0.3" />
                    </effect>
                </Button>
                <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0" text="=" textAlignment="JUSTIFY" GridPane.columnIndex="3" GridPane.rowIndex="4" onAction="#result">
