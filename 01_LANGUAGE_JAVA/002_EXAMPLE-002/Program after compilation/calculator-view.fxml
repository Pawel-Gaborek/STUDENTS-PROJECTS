                    </font>
                    <cursor>
                        <Cursor fx:constant="DEFAULT" />
                    </cursor>
                    <effect>
                        <Blend mode="DIFFERENCE" opacity="0.3" />
                    </effect>
                </Button>
                <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0" text="Del" GridPane.columnIndex="1" GridPane.rowIndex="4" onAction="#delete">
