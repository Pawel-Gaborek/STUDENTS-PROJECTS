                    <font>
                        <Font size="18.0" />
                    </font>
                </Button>
                <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0" text="," GridPane.columnIndex="2" GridPane.rowIndex="4" onAction="#buttonComma">
